<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- API 14 theme customizations can go here. -->
    </style>

    <style name="Theme.Default" parent="@android:style/Theme.Holo.Light"/>

    <style name="Theme.NoTitle" parent="@android:style/Theme.Holo.Light.NoActionBar"/>

    <style name="Theme.FullScreen" parent="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>

    <style name="Theme.Dialog.TittleCenter" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:gravity">center</item>
    </style>

</resources>